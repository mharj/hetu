{
    "name": "fi-pin",
    "version": "0.0.5",
    "description": "Finnish Personal Identification Number validate + gender check",
    "main": "dist/index.js",
    "devDependencies": {
        "@types/chai": "^4.2.12",
        "@types/mocha": "^8.0.3",
        "@types/node": "^12.12.50",
        "chai": "^4.2.0",
        "mocha": "^8.1.3",
        "mocha-junit-reporter": "^2.0.0",
        "nyc": "^15.1.0",
        "source-map-support": "^0.5.19",
        "ts-node": "^9.0.0",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^4.0.3"
    },
    "scripts": {
        "build": "tsc",
        "prepublishOnly": "npm run test && npm run build",
        "test": "nyc mocha"
    },
    "mocha": {
        "exit": true,
        "extension": [
            "ts",
            "js"
        ],
        "recursive": true,
        "require": [
            "ts-node/register",
            "source-map-support/register"
        ],
        "reporters": [
            "spec",
            "mocha-junit-reporter"
        ]
    },
    "keywords": [
        "pin",
        "personid"
    ],
    "files": [
        "dist"
    ],
    "nyc": {
        "extension": [
            ".ts"
        ],
        "include": [
            "src"
        ],
        "reporter": [
            "text",
            "lcovonly"
        ],
        "all": true
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mharj/hetu.git"
    },
    "author": "mharj",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/mharj/hetu/issues"
    },
    "homepage": "https://github.com/mharj/hetu#readme"
}