{
  "name": "fi-pin",
  "version": "0.0.4",
  "description": "Finnish Personal Identification Number validate + gender check",
  "main": "dist/index.js",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^12.12.44",
    "chai": "^4.2.0",
    "mocha": "^7.2.0",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.5"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run test && npm run build",
    "test": "mocha test/*.ts",
    "coverage": "nyc mocha test/*.ts"
  },
  "mocha": {
    "exit": true,
    "recursive": true,
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "keywords": [
    "pin",
    "personid"
  ],
  "files": [
    "dist"
  ],
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mharj/hetu.git"
  },
  "author": "mharj",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mharj/hetu/issues"
  },
  "homepage": "https://github.com/mharj/hetu#readme"
}
