{
    "name": "fi-pin",
    "version": "0.0.9",
    "description": "Finnish Personal Identification Number validate + gender check",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js",
            "import": "./dist/index.mjs"
        }
    },
    "devDependencies": {
        "@biomejs/biome": "2.3.8",
        "@types/node": "^25.0.0",
        "@vitest/coverage-v8": "^4.0.15",
        "c8": "^10.1.3",
        "tsup": "^8.5.1",
        "typescript": "^5.9.3",
        "vitest": "^4.0.15",
        "zod": "^4.1.13"
    },
    "scripts": {
        "build": "tsup src/index.ts --sourcemap --format cjs,esm --dts --clean",
        "prepublishOnly": "npm run test && npm run build",
        "test": "vitest test --typecheck --run --no-isolate --coverage",
        "coverage": "vitest test --typecheck --run --no-isolate --reporter=dot --coverage --coverage.reporter=lcov",
        "lint": "biome check",
        "validate": "tsc --noEmit"
    },
    "mocha": {
        "exit": true,
        "extension": [
            "ts",
            "js"
        ],
        "recursive": true,
        "require": [
            "ts-node/register",
            "source-map-support/register"
        ],
        "reporters": [
            "spec",
            "mocha-junit-reporter"
        ]
    },
    "keywords": [
        "pin",
        "personid"
    ],
    "files": [
        "dist"
    ],
    "nyc": {
        "extension": [
            ".ts"
        ],
        "include": [
            "src"
        ],
        "reporter": [
            "text",
            "lcovonly"
        ],
        "all": true
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mharj/hetu.git"
    },
    "author": "mharj",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/mharj/hetu/issues"
    },
    "homepage": "https://github.com/mharj/hetu#readme",
    "packageManager": "pnpm@9.12.3+sha512.cce0f9de9c5a7c95bef944169cc5dfe8741abfb145078c0d508b868056848a87c81e626246cb60967cbd7fd29a6c062ef73ff840d96b3c86c40ac92cf4a813ee"
}
